  .386
.model flat,stdcall
.stack 4096
ExitProcess PROTO, dwExitCode:DWORD

.data
x1 DWORD 12
x2 DWORD 10
x3 DWORD 1
x4 DWORD 9

.code
main proc

;x1 < x2 > x3 < x4
	mov eax, x1
	cmp eax, x2
	jnge Dox3
	mov eax, x1
	XCHG eax, x2
	mov x1, eax

Dox3:
	mov eax, x3
	cmp eax, x2
	jnge Dox4
	mov eax, x3
	XCHG eax, x2
	mov x3, eax

Dox4:
	mov eax, x4
	cmp eax, x3
	jnge DoEnd
	mov eax, x4
	XCHG eax, x3
	mov x4, eax

DoEnd:
	INVOKE ExitProcess,0
	main ENDP
	END main
